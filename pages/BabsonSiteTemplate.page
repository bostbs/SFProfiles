<apex:page showHeader="false" controller="CORECONNECT.CC_CommunityTemplateController" sidebar="false" standardStylesheets="false" applyHtmlTag="false" applyBodyTag="false" docType="html-5.0">
    <!-- [if lt IE 7]>      <html lang="en" class="no-js lt-ie10 lt-ie9 lt-ie8 lt-ie7"> <![endif] -->
    <!-- [if IE 7]>         <html lang="en" class="no-js lt-ie10 lt-ie9 lt-ie8"> <![endif] -->
    <!-- [if IE 8]>         <html lang="en" class="no-js lt-ie10 lt-ie9"> <![endif] -->
    <!-- [if IE 9]>         <html lang="en" class="no-js lt-ie10"> <![endif] -->
    <!-- [if gt IE 9]><! -->
    <html lang="en" class="no-js">
        <!-- <![endif] -->
        <head>
            <meta charset="utf-8" />
            <title>
                <apex:insert name="pagetitle"></apex:insert>
            </title>
            <meta http-equiv="X-UA-Compatible" content="IE=edge" />
            <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no" />
            <link rel="shortcut icon" href="{!FaviconURL}" type="image/x-icon" />
            <meta name="apple-mobile-web-app-capable" content="yes" />
            <link rel="apple-touch-icon" sizes="152x152" href="{!FaviconURL}" />
            <style><CORECONNECT:CC_CommunityCSSMain /></style> 
            <link rel="stylesheet" href="{!URLFOR($Page.CORECONNECT__CC_Community_MainBrandingCSS)}" />
            <apex:outputPanel layout="none" rendered="{!NOT(ISBLANK(HeaderCSSBrandingResourceName))}">
                <link rel="stylesheet" type="text/css" href="{!HeaderCSSBrandingResourceName}"/>
            </apex:outputPanel>
            <script src="{!URLFOR($Resource.CORECONNECT__CC_Community, 'script/respond.min.js')}"></script>
            <script src="{!URLFOR($Resource.CORECONNECT__CC_Community, 'script/modernizr-2.6.2.min.js')}"></script>
            <script src="{!URLFOR($Resource.CORECONNECT__CC_Community, 'script/jquery-1.10.2.min.js')}"></script>
            
            <c:BabsonFonts />
           
        </head>
        <body class="branding">
            <a class="sr-only sr-only-focusable" href="#content">Skip to main content</a>
            <!-- header -->
            <apex:insert name="header">
                <c:BabsonHeader />
                
            </apex:insert>
            <!-- body -->
            <div id="content">
                <apex:insert name="body">
                    <div id="wrap" class="container-fluid"></div>
                </apex:insert>
            </div>
            
            <!-- footer -->
            <apex:insert name="footer">
                <c:BabsonFooter />
            </apex:insert>
            <script src="{!URLFOR($Resource.CORECONNECT__CC_Community, 'script/main.min.js')}"></script>
            <script src="{!URLFOR($Resource.CORECONNECT__CC_Community, 'script/source/ChatterGroupProfilePage.js')}"></script>
            <script src="{!URLFOR($Resource.Google, 'script/gtm.js')}"></script>
            <!-- Javascript to be appended to the footer -->
            <apex:insert name="footerjs"></apex:insert>
        </body>
    </html>
</apex:page>