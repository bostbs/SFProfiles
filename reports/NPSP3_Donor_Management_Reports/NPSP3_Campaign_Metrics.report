        <aggregateTypes>Minimum</aggregateTypes>
        <field>NUM_WON_OPPORTUNITIES</field>
    </columns>
    <columns>
        <field>NUM_CONTACTS</field>
    </columns>
    <description>Campaign Metrics</description>
    <filter>
