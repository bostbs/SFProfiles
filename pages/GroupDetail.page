<apex:page showHeader="false" sidebar="false" standardStylesheets="false" cache="false" applyHtmlTag="false" applyBodyTag="false" docType="html-5.0" controller="CORECONNECT.CC_CommunityGroupDetailController" >
	<apex:composition template="{!$Site.Template}">
		<apex:define name="pagetitle">{!objCutsomGroup.Name}</apex:define>
		<apex:define name="body" >
			<style>.fontButton {
            font-size: large;
        }</style>
			<header class="container page-heading">
				<apex:pageMessages ></apex:pageMessages>
				<apex:form >
					<apex:outputPanel id="JoinLeaveButtons" >
						<apex:commandLink id="joinButton" rerender="JoinLeaveButtons" action="{!Join}" rendered="{!CGM==null && IsOwner==false && CollabGroup.CollaborationType=='Public'}" styleClass="btn btn-info btn-lg pull-right hidden-xs">{!$Label.CORECONNECT__CC_Group_Detail_Join}</apex:commandLink>
						<apex:commandLink id="leaveButton" rerender="JoinLeaveButtons" action="{!LeaveGroup}" rendered="{!CGM!=null && IsOwner==false}" styleClass="btn btn-info btn-lg pull-right hidden-xs">{!$Label.CORECONNECT__CC_Group_Detail_Leave}</apex:commandLink>
						<apex:commandLink rendered="{!IsOwner==true}" styleClass="btn btn-info btn-lg pull-right hidden-xs disabled">{!$Label.CORECONNECT__CC_Group_Detail_Owner}</apex:commandLink>
					</apex:outputPanel>
				</apex:form>
				<h1 class="inline group-name">
					<apex:outputField value="{!objCutsomGroup.Name}" id="groupNameField" ></apex:outputField>
					<span class="label label-default">{!CollabGroup.CollaborationType}</span>
					<apex:outputPanel rendered="{!isOwner || isManager}">
						<div class="edit">
							<button class="close" data-toggle="modal" data-target="#group-name-edit">{!$Label.CORECONNECT__CC_Group_Detail_Edit_Name}</button>
						</div>
					</apex:outputPanel>
				</h1>
			</header>
			<article class="container box group-detail">
				<div id="bannerImage" class="group-header " style="background-image: url({!If(BannerImageAttachment!=null,URLFOR($Action.Attachment.Download, BannerImageAttachment.Id),URLFOR($Resource.CORECONNECT__CC_Community, 'images/bg-group-default.png'))})">
					<apex:outputPanel rendered="{!isOwner || isManager}">
						<button class="edit close" data-toggle="modal" data-target="#banner-edit">{!$Label.CORECONNECT__CC_Group_Detail_Edit}</button>
					</apex:outputPanel>
					<div class="shading">
						<div class="avatar ">
							<apex:outputPanel rendered="{!CONTAINS(CollabGroup.FullPhotoUrl, '0F9/F')}">
								<img src="{!DefaultGroupLogo}" alt="{!objCutsomGroup.Name}" />
							</apex:outputPanel>
							<apex:outputPanel rendered="{!NOT(CONTAINS(CollabGroup.FullPhotoUrl, '0F9/F'))}">
								<img src="{!CollabGroup.FullPhotoUrl}" alt="{!objCutsomGroup.Name}" />
							</apex:outputPanel>
							<apex:outputPanel rendered="{!isOwner || isManager}">
								<button class="edit close" data-toggle="modal" data-target="#group-profile-image-edit">{!$Label.CORECONNECT__CC_Group_Detail_Edit}</button>
							</apex:outputPanel>
						</div>
						<ul class="nav nav-tabs">
							<li class="active">
								<a href="#">{!$Label.CORECONNECT__CC_Group_Detail_Overview}</a>
							</li>
							<li>
								<a href="{!$Site.BaseUrl}/_ui/core/chatter/content/GroupFileListPage?g={!objCutsomGroup.CORECONNECT__Collaboration_GroupId__c}">{!$Label.CORECONNECT__CC_Group_Detail_Files}</a>
							</li>
							<li>
								<a href="javascript:void(0);" onclick="if(!window.groupMembersDialog) { window.groupMembersDialog = chatter.createModalOverlay({id:'groupMembersDialog',height:'420',title:'Members',url:'/ui/groups/{!JSENCODE(objCutsomGroup.CORECONNECT__Collaboration_GroupId__c)}/memberoverlay',params:{filter:'member'}}); window.groupMembersDialog.register(); } window.groupMembersDialog.open();" class="dialog_groupMembersDialog">
									<span id="moreGroupMembersLink">{!$Label.CORECONNECT__CC_Group_Detail_Members}</span>
								</a>
							</li>
							<apex:outputPanel id="JoinLeaveButtons" layout="none">
								<li class="pull-right visible-xs">
									<apex:form >
										<apex:commandLink id="joinButton" rerender="JoinLeaveButtons" action="{!Join}" rendered="{!CGM==null && IsOwner==false && CollabGroup.CollaborationType=='Public'}">{!$Label.CORECONNECT__CC_Group_Detail_Join}</apex:commandLink>
										<apex:commandLink id="leaveButton" rerender="JoinLeaveButtons" action="{!LeaveGroup}" rendered="{!CGM!=null && IsOwner==false}">{!$Label.CORECONNECT__CC_Group_Detail_Leave}</apex:commandLink>
									</apex:form>
								</li>
							</apex:outputPanel>
							<apex:outputPanel rendered="{!hasAccess}" layout="none">
								<li class="pull-right hidden-xs">
									<a href="javascript:HelpBubble.hideBubble(HelpBubbleConstants.CLASS_CHANGE_EMAIL_SETTINGS_HELP_BUBBLE);sfdcPage.openPersonalSetup(%27{!$Site.Prefix}/_ui/common/setup/PersonalSetup%27,%27Notifications%27,%27{!objCutsomGroup.CORECONNECT__Collaboration_GroupId__c}KAM%27);" class="emailSettingsLinkItem">{!$Label.CORECONNECT__CC_Group_Detail_Email_Settings}</a>
								</li>
							</apex:outputPanel>
						</ul>
					</div>
				</div>
				<apex:outputPanel rendered="{!AND(OR(objCutsomGroup.CORECONNECT__Group_Text_Area_1__c != '', objCutsomGroup.CORECONNECT__Group_Text_Area_2__c != '', objCutsomGroup.CORECONNECT__Group_Text_Area_3__c != '', objCutsomGroup.CORECONNECT__Group_Summary_Title__c != '', objCutsomGroup.CORECONNECT__Group_Summary__c != '', isOwner || isManager), hasAccess)}" layout="none">
					<apex:outputPanel styleClass="text-center" rendered="true" layout="none">
						<div class="col-xs-0 group-subtext">
							<apex:outputPanel rendered="{!isOwner || isManager}">
								<button class="edit close fontButton" data-toggle="modal" data-target="#news-summary-edit">{!$Label.CORECONNECT__CC_Group_Detail_Edit}</button>
							</apex:outputPanel>
							<h1>
								<apex:outputText id="upSummaryTitle" value="{!HTMLENCODE(objCutsomGroup.CORECONNECT__Group_Summary_Title__c)}"/>
							</h1>
							<apex:outputField id="upSummary" value="{!objCutsomGroup.CORECONNECT__Group_Summary__c}"/>
						</div>
					</apex:outputPanel>
					<section class="row group-news-items">
						<!-- Updates 1 -->
						<apex:outputPanel rendered="{!OR(objCutsomGroup.CORECONNECT__Group_Text_Area_1_Title__c != '', isOwner || isManager)}" layout="none">
							<div class="col-sm-4 group-news-item ">
								<apex:outputPanel rendered="{!isOwner || isManager}">
									<button class="edit close fontButton" data-toggle="modal" data-target="#news-item-1-edit">{!$Label.CORECONNECT__CC_Group_Detail_Edit}</button>
								</apex:outputPanel>
								<h3>
									<apex:outputText id="upTitle1" value="{!IF(objCutsomGroup.CORECONNECT__Group_Text_Area_1_Title__c == '', 'Default Title', objCutsomGroup.CORECONNECT__Group_Text_Area_1_Title__c)}" ></apex:outputText>
								</h3>
								<apex:outputField id="upBody1" value="{!objCutsomGroup.CORECONNECT__Group_Text_Area_1__c}"/>
							</div>
							<hr class="visible-xs" />
						</apex:outputPanel>
						<!-- Updates 2 -->
						<apex:outputPanel rendered="{!OR(objCutsomGroup.CORECONNECT__Group_Text_Area_2_Title__c != '', isOwner || isManager)}" layout="none">
							<div class="col-sm-4 group-news-item ">
								<apex:outputPanel rendered="{!isOwner || isManager}">
									<button class="edit close fontButton" data-toggle="modal" data-target="#news-item-2-edit">{!$Label.CORECONNECT__CC_Group_Detail_Edit}</button>
								</apex:outputPanel>
								<h3>
									<apex:outputText id="upTitle2" value="{!IF(objCutsomGroup.CORECONNECT__Group_Text_Area_2_Title__c == '', 'Default Title', objCutsomGroup.CORECONNECT__Group_Text_Area_2_Title__c)}"></apex:outputText>
								</h3>
								<apex:outputField id="upBody2" value="{!objCutsomGroup.CORECONNECT__Group_Text_Area_2__c}"/>
							</div>
							<hr class="visible-xs" />
						</apex:outputPanel>
						<!-- Updates 3 -->
						<apex:outputPanel rendered="{!OR(objCutsomGroup.CORECONNECT__Group_Text_Area_3_Title__c != '', isOwner || isManager)}" layout="none">
							<div class="col-sm-4 group-news-item ">
								<apex:outputPanel rendered="{!isOwner || isManager}">
									<button class="edit close fontButton" data-toggle="modal" data-target="#news-item-3-edit">{!$Label.CORECONNECT__CC_Group_Detail_Edit}</button>
								</apex:outputPanel>
								<h3>
									<apex:outputText id="upTitle3" value="{!IF(objCutsomGroup.CORECONNECT__Group_Text_Area_3_Title__c == '', 'Default Title', objCutsomGroup.CORECONNECT__Group_Text_Area_3_Title__c)}"></apex:outputText>
								</h3>
								<apex:outputField id="upBody3" value="{!objCutsomGroup.CORECONNECT__Group_Text_Area_3__c}"/>
							</div>
							<hr class="visible-xs" />
						</apex:outputPanel>
					</section>
					<hr class="hidden-xs" />
				</apex:outputPanel>
				<!-- Description, Important Link, Open Text -->
				<div class="row">
					<div class="group-sidebar col-sm-4 col-sm-push-8 col-md-3 col-md-push-9">
						<!-- Description -->
						<apex:outputPanel rendered="{!AND(OR(CollabGroup.Description != '', isOwner || isManager), hasAccess)}" layout="none">
							<section class="group-sidebar-item ">
								<apex:outputPanel rendered="{!isOwner || isManager}">
									<button class="edit close fontButton" data-toggle="modal" data-target="#sidebar-item-0-edit">{!$Label.CORECONNECT__CC_Group_Detail_Edit}</button>
								</apex:outputPanel>
								<h3>{!$Label.CORECONNECT__CC_Group_Detail_Description}</h3>
								<apex:outputText id="description" value="{!HTMLENCODE(CollabGroup.Description)}" />
							</section>
						</apex:outputPanel>
						<!-- Category -->
						<apex:outputPanel rendered="{!AND(OR(objCutsomGroup.CORECONNECT__Category__c != '', isOwner || isManager), hasAccess)}" layout="none">
							<section class="group-sidebar-item ">
								<apex:outputPanel rendered="{!isOwner || isManager}">
									<button class="edit close fontButton " data-toggle="modal" data-target="#group-category-edit">{!$Label.CORECONNECT__CC_Group_Detail_Edit}</button>
								</apex:outputPanel>
								<h3>{!$Label.CORECONNECT__CC_Group_Detail_Category}</h3>
								<apex:outputText id="category" value="{!HTMLENCODE(objCutsomGroup.CORECONNECT__Category__c)}" />
							</section>
						</apex:outputPanel>
						<!-- Important Links -->
						<apex:outputPanel rendered="{!AND(OR(objCutsomGroup.CORECONNECT__Important_Links__c != '', isOwner || isManager), hasAccess)}" layout="none">
							<section class="group-sidebar-item ">
								<apex:outputPanel rendered="{!isOwner || isManager}" layout="none">
									<button class="edit fontButton close" data-toggle="modal" data-target="#sidebar-item-1-edit">{!$Label.CORECONNECT__CC_Group_Detail_Edit}</button>
								</apex:outputPanel>
								<h3>{!$Label.CORECONNECT__CC_Group_Detail_Links}</h3>
								<apex:outputField id="importantLink" value="{!objCutsomGroup.CORECONNECT__Important_Links__c}" />
							</section>
						</apex:outputPanel>
						<!-- Open Text Area -->
						<apex:outputPanel rendered="{!AND(OR(objCutsomGroup.CORECONNECT__Open_Text__c != '', isOwner || isManager), hasAccess)}" layout="none">
							<section class="group-sidebar-item ">
								<apex:outputPanel rendered="{!isOwner || isManager}">
									<button class="edit close fontButton " data-toggle="modal" data-target="#sidebar-item-2-edit">{!$Label.CORECONNECT__CC_Group_Detail_Edit}</button>
								</apex:outputPanel>
								<h3>
									<apex:outputText id="openTextTitle" value="{!IF(objCutsomGroup.CORECONNECT__Open_Text_Title__c=='','Default Open Text',objCutsomGroup.CORECONNECT__Open_Text_Title__c)}"></apex:outputText>
								</h3>
								<apex:outputText id="openTextBody"  value="{!objCutsomGroup.CORECONNECT__Open_Text__c}"/>
							</section>
						</apex:outputPanel>
						<apex:outputPanel rendered="{!isOwner || isManager}" layout="none">
							<section class="group-sidebar-item ">
								<h3>{!$Label.CORECONNECT__CC_Group_Detail_Group_Management}</h3>
								<ul class="list-unstyled">
									<apex:outputPanel layout="none" rendered="{!CollabGroup.CollaborationType=='Private' && NumberRequested >0}">
										<li class="manage-requests-link">
											<a href="javascript:void(0);" id="manageRequestsLink" onclick="if(!window.groupUserManagementDialog) {window.groupUserManagementDialog = new GroupPanelAccessibleUrlDialog({dialogId:'groupUserManagementDialog',dialogTitle:'{!$Label.CORECONNECT__CC_Group_Detail_Requests_Add_Members}',url:'/ui/groups/{!JSENCODE(objCutsomGroup.CORECONNECT__Collaboration_GroupId__c)}/memberaddremove',subjectId:'{!objCutsomGroup.CORECONNECT__Collaboration_GroupId__c}',height:'0',onHide:function() { SfdcApp.ChatterGroupAddRemoveMemberOverlay.resetRequestConfig() }});window.groupUserManagementDialog.width=500;window.groupUserManagementDialog.register();}window.groupUserManagementDialog.open({params:{'filter':'requests'},onSuccess:function(json) { SfdcApp.ChatterGroupAddRemoveMemberOverlay.setRequestConfig('requests'); SfdcApp.ChatterGroupAddRemoveMemberOverlay.processPayload(json.payload);}});" title="{!$Label.CORECONNECT__CC_Group_Detail_Requests_Title}">{!$Label.CORECONNECT__CC_Group_Detail_Requests}</a>
										</li>
									</apex:outputPanel>
									<li>
										<a href="javascript:void(0);" id="manageUsersLink" onclick="if(!window.groupUserManagementDialog) {window.groupUserManagementDialog = new GroupPanelAccessibleUrlDialog({dialogId:'groupUserManagementDialog',dialogTitle:'{!$Label.CORECONNECT__CC_Group_Detail_Add_Remove_Members}',url:'/ui/groups/{!JSENCODE(objCutsomGroup.CORECONNECT__Collaboration_GroupId__c)}/memberaddremove',subjectId:'{!objCutsomGroup.CORECONNECT__Collaboration_GroupId__c}',height:'0',onHide:function() { SfdcApp.ChatterGroupAddRemoveMemberOverlay.resetRequestConfig() }});window.groupUserManagementDialog.width=500;window.groupUserManagementDialog.register();}window.groupUserManagementDialog.open({params:{'filter':'all'},onSuccess:function() { SfdcApp.ChatterGroupAddRemoveMemberOverlay.setRequestConfig('all');}});">{!$Label.CORECONNECT__CC_Group_Detail_Add_Remove_Members}</a>
									</li>
									<li>
										<a href="javascript:void(0);" id="changeGroupRolesLink" onclick="if(!window.groupRolesDialog) {window.groupRolesDialog = new GroupPanelAccessibleUrlDialog({dialogId:'groupRolesDialog',dialogTitle:'{!$Label.CORECONNECT__CC_Group_Detail_Change_Roles}',url:'/ui/groups/{!objCutsomGroup.CORECONNECT__Collaboration_GroupId__c}/changeroles',subjectId:'{!objCutsomGroup.CORECONNECT__Collaboration_GroupId__c}',height:'0',onHide:function() { SfdcApp.ChatterGroupChangeRolesOverlay.resetRequestConfig() }});window.groupRolesDialog.width=500;window.groupRolesDialog.register();}window.groupRolesDialog.open({params:{'filter':'member'}});">{!$Label.CORECONNECT__CC_Group_Detail_Change_Roles}</a>
									</li>
									<apex:outputPanel layout="none" rendered="{!isOwner}">
										<li>
											<a data-toggle="modal" data-target="#group-delete" href="#">{!$Label.CORECONNECT__CC_Group_Detail_Delete_Group}</a>
										</li>
									</apex:outputPanel>
								</ul>
							</section>
						</apex:outputPanel>
					</div>
					<section id="comments" class="group-chatter comments col-sm-8 col-sm-pull-4 col-md-9 col-md-pull-3">
						<hr class="visible-xs" />
						<h2>
							<apex:outputField id="groupNameField2" value="{!objCutsomGroup.Name}"></apex:outputField>&nbsp;{!$Label.CORECONNECT__CC_Group_Detail_Feed}
						</h2>
						<chatter:feed rendered="{!hasAccess}" entityId="{!objCutsomGroup.CORECONNECT__Collaboration_GroupId__c}" />
					</section>
				</div>
			</article>
			<!-- Assorted Modal Windows needed for the edit functionality above. Only needed if the user is an admin. -->
			<!-- Delete This Group -->
			<div class="modal fade" id="group-delete" tabindex="-1" role="dialog" aria-labelledby="group-delete" aria-hidden="true">
				<div class="modal-dialog">
					<apex:form >
						<form role="form">
							<div class="modal-content">
								<div class="modal-header">
									<button type="button" class="close" data-dismiss="modal">
										<span aria-hidden="true">&times;</span>
										<span class="sr-only">{!$Label.CORECONNECT__CC_Group_Detail_Close}</span>
									</button>
									<h4 class="modal-title" id="group-label">{!$Label.CORECONNECT__CC_Group_Detail_Delete_Group}</h4>
								</div>
								<div class="modal-body">
									<div class="form-group">{!$Label.CORECONNECT__CC_Group_Detail_Delete_Group_Statement}&nbsp;
										<apex:outputField id="groupNameField2" value="{!objCutsomGroup.Name}"></apex:outputField>{!$Label.CORECONNECT__CC_Group_Detail_Question_Mark}
									</div>
								</div>
								<div class="modal-footer">
									<apex:commandButton value="{!$Label.CORECONNECT__CC_Group_Detail_Close}" styleClass="btn btn-default" html-data-dismiss="modal"></apex:commandButton>
									<apex:commandButton html-data-dismiss="modal" value="{!$Label.CORECONNECT__CC_Group_Detail_Delete}" action="{!DeleteThisGroup}" styleClass="btn btn-primary" rerender="groupNameField" ></apex:commandButton>
								</div>
							</div>
						</form>
					</apex:form>
				</div>
			</div>
			<!-- Group Name -->
			<div class="modal fade" id="group-name-edit" tabindex="-1" role="dialog" aria-labelledby="group-name-label" aria-hidden="true">
				<div class="modal-dialog">
					<apex:form >
						<form role="form">
							<div class="modal-content">
								<div class="modal-header">
									<button type="button" class="close" data-dismiss="modal">
										<span aria-hidden="true">&times;</span>
										<span class="sr-only">{!$Label.CORECONNECT__CC_Group_Detail_Close}</span>
									</button>
									<h4 class="modal-title" id="group-label">{!$Label.CORECONNECT__CC_Group_Detail_Edit_Group_Name}</h4>
								</div>
								<div class="modal-body">
									<div class="form-group">
										<apex:inputField label="Group Name:" styleClass="form-control" value="{!objCutsomGroup.Name}"></apex:inputField>
									</div>
								</div>
								<div class="modal-footer">
									<apex:commandButton value="{!$Label.CORECONNECT__CC_Group_Detail_Close}" styleClass="btn btn-default" html-data-dismiss="modal"></apex:commandButton>
									<apex:commandButton html-data-dismiss="modal" value="{!$Label.CORECONNECT__CC_Group_Detail_Save}" action="{!EditGroupName}" styleClass="btn btn-primary" rerender="groupNameField,groupNameField2" ></apex:commandButton>
								</div>
							</div>
						</form>
					</apex:form>
				</div>
			</div>
			<!-- Banner Image -->
			<div class="modal fade" id="banner-edit" tabindex="-1" role="dialog" aria-labelledby="banner-label" aria-hidden="true">
				<div class="modal-dialog">
					<apex:form >
						<form role="form">
							<div class="modal-content">
								<div class="modal-header">
									<button type="button" class="close" data-dismiss="modal">
										<span aria-hidden="true">&times;</span>
										<span class="sr-only">{!$Label.CORECONNECT__CC_Group_Detail_Close}</span>
									</button>
									<h4 class="modal-title" id="group-label">{!$Label.CORECONNECT__CC_Group_Detail_Upload_Banner_Image}</h4>
									<h4 class="modal-title" id="group-label">Maximum size of image must be equal or less than 1645x250</h4>
								</div>
								<div class="modal-body">
									<div class="form-group">
										<apex:inputFile value="{!NewBannerImageAttachment.body}" filename="{!NewBannerImageAttachment.name}" styleClass="file image" accept="gif,png,jpg,jpeg" />
									</div>
								</div>
								<div class="modal-footer">
									<apex:commandButton value="{!$Label.CORECONNECT__CC_Group_Detail_Close}" styleClass="btn btn-default" html-data-dismiss="modal" ></apex:commandButton>
									<apex:commandButton value="{!$Label.CORECONNECT__CC_Group_Detail_Save}" action="{!UploadNewBannerImage}" styleClass="btn btn-primary" ></apex:commandButton>
								</div>
							</div>
						</form>
					</apex:form>
				</div>
			</div>
			<!-- Profile Image -->
			<div class="modal fade" id="group-profile-image-edit" tabindex="-1" role="dialog" aria-labelledby="group-profile-image-label" aria-hidden="true">
				<div class="modal-dialog">
					<apex:form >
						<form role="form">
							<div class="modal-content">
								<div class="modal-header">
									<button type="button" class="close" data-dismiss="modal">
										<span aria-hidden="true">&times;</span>
										<span class="sr-only">{!$Label.CORECONNECT__CC_Group_Detail_Close}</span>
									</button>
									<h4 class="modal-title" id="group-label">{!$Label.CORECONNECT__CC_Group_Detail_Upload_Profile_Image}</h4>
									<h4 class="modal-title" id="group-label">Label.CORECONNECT__CC_Group_Detail_Upload_Profile_Image_Max_Size</h4>
								</div>
								<div class="modal-body">
									<div class="form-group">
										<apex:inputFile value="{!GroupProfilePicBody}" filename="{!GroupProfilePicName}" contentType="{!GroupProfilePicContType}" styleClass="file image" accept="gif,png,jpg,jpeg" />
										<div class="help-block">{!$Label.CORECONNECT__CC_Group_Detail_Upload_Profile_Image_Help}</div>
									</div>
								</div>
								<div class="modal-footer">
									<apex:commandButton value="{!$Label.CORECONNECT__CC_Group_Detail_Close}" styleClass="btn btn-default" html-data-dismiss="modal" ></apex:commandButton>
									<apex:commandButton value="{!$Label.CORECONNECT__CC_Group_Detail_Save}" styleClass="btn btn-primary" action="{!UploadGroupProfileImage}" ></apex:commandButton>
								</div>
							</div>
						</form>
					</apex:form>
				</div>
			</div>
			<!-- Group Summary -->
			<div class="modal fade" id="news-summary-edit" tabindex="-1" role="dialog" aria-labelledby="news-summary-label" aria-hidden="true">
				<div class="modal-dialog">
					<apex:form >
						<form role="form">
							<div class="modal-content">
								<div class="modal-header">
									<button type="button" class="close" data-dismiss="modal">
										<span aria-hidden="true">&times;</span>
										<span class="sr-only">{!$Label.CORECONNECT__CC_Group_Detail_Close}</span>
									</button>
									<h4 class="modal-title" id="news-item-1-label">{!$Label.CORECONNECT__CC_Group_Detail_Edit_Summary}</h4>
								</div>
