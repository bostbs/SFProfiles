<apex:page showHeader="false" 
			sidebar="false" 
			standardStylesheets="false" 
			applyHtmlTag="false" 
			applyBodyTag="false" 
			docType="html-5.0" 
			controller="Level2TemplateController"
			>

	<apex:composition template="{!$Site.Template}">
